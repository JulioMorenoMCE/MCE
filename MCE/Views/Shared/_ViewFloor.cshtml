@model MCE.Models.tblFloor
@using (@Html.BeginForm("SaveFloor", "App", FormMethod.Post, new { @id = "SaveFloorForm", enctype = "multipart/form-data", @data_toggle = "validator" }))
{
    @Html.AntiForgeryToken()
    <div class="">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="col-sm-12 nopadding">
            <div class="col-sm-12">
                <label class="col-sm-6">Job:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.Job)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">Technician:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.Technician)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">Stage:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.Stage)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">Status:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.Status)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">CAT:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.CAT)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">ST:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.ST)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">TTC:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.TTC)</div>
            </div>
            <div class="clearfix"></div>
            <br />
            <div class="col-sm-12">
                <label class="col-sm-6">Description:</label>
                <div class="col-sm-6">@Html.DisplayFor(model => model.description)</div>
            </div>
            <br />
            <div class="col-sm-12">
                <div class="pull-right">
                    <button type="button" class="btn btn-white waves-light" onclick="CloseModal('view-modal')">Close</button>&nbsp;
                </div>
            </div>
        </div>
    </div>
}
